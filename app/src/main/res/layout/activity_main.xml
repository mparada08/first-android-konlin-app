<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="pl.net.parada.dietplus.MainActivity">


    <com.github.florent37.materialviewpager.MaterialViewPager
            android:id="@+id/materialviewpager"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:viewpager_animatedHeaderImage="true"
            app:viewpager_displayToolbarWhenSwipe="true"
            app:viewpager_enableToolbarElevation="true"
            app:viewpager_headerAdditionalHeight="20dp"
            app:viewpager_headerAlpha="1.0"
            app:viewpager_headerHeight="200dp"
            app:viewpager_hideLogoWithFade="false"
            app:viewpager_hideToolbarAndTitle="true"
            app:viewpager_imageHeaderDarkLayerAlpha="0.2"
            app:viewpager_logoMarginTop="100dp"
            app:viewpager_logo="@layout/logo_layout"
            app:viewpager_pagerTitleStrip="@layout/material_view_pager_pagertitlestrip_fullsize"
            app:viewpager_parallaxHeaderFactor="1.5"
            app:viewpager_transparentToolbar="false"

    >


    </com.github.florent37.materialviewpager.MaterialViewPager>

</android.support.constraint.ConstraintLayout>
